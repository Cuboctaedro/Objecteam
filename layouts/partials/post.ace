section class="w-100 md:w-50p mb-6 px-gutter"
  div class="p-4 card"
    h2 class="title" 
      a href={{ .URL | absLangURL }} {{ .Title }}
    div class="my-2 date"
      {{ partial "date" . }}
    div class="my-2" {{ .Summary }}
    a href={{ .URL | absLangURL }} class="btn btn-acc" {{ i18n "read_more"}}